{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug-rclcpp(gbd)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/install/autol_pointcloud/lib/autol_pointcloud/autol_pointcloud_node",
            "args": [],
            "preLaunchTask": "colcon: build",
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}